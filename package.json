{
  "name": "@apigear-io/epic-blueprint",
  "version": "1.15.4",
  "private": true,
  "description": "An unreal blueprint code generator",
  "keywords": [
    "objectapi",
    "epic",
    "unreal",
    "blueprint",
    "c++",
    "library",
    "client"
  ],
  "devDependencies": {
    "@apigear-io/objectapi-cli": "^1.15.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "gen:goldenmaster": "rm -rf goldenmaster && node node_modules/@apigear-io/objectapi-cli/bin/apigear-gen --solution apigear/goldenmaster.solution.yaml && mv test/ goldenmaster",
    "test:goldenmaster": "rm -rf test && node node_modules/@apigear-io/objectapi-cli/bin/apigear-gen --solution apigear/goldenmaster.solution.yaml && diff -r goldenmaster/ test/",
    "report:license": "license-report --exclude=@apigear-io/objectapi-cli --department.value=apigear --relatedTo.value=$npm_package_name --output=csv --csvHeaders > licenses.csv",
    "version": "standard-version -i CHANGELOG.md --same-file"
  },
  "author": {
    "name": "ApiGear UG",
    "url": "https://github.com/apigear-io/template-unreal"
  },
  "homepage": "https://github.com/apigear-io/objectapi-generator#readme",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "files": [
    "/templates",
    "/rules.yaml",
    "/README.md",
    "/LICENSE"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apigear-io/template-unreal.git"
  },
  "bugs": {
    "url": "https://github.com/apigear-io/template-unreal/issues"
  }
}
